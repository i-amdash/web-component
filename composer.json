{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "landmark/landmark-web-component",
    "type": "library",
    "description": "A reusable Laravel Livewire contact form component package",
    "keywords": ["laravel", "livewire", "contact-form", "component", "web-component", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ashimi Doyin",
            "email": "ashimidoyin@yahoo.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "livewire/livewire": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Landmark\\LandmarkWebComponent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Landmark\\LandmarkWebComponent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Landmark\\LandmarkWebComponent\\LandmarkWebComponentServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}